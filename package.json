{
  "name": "faqs-microservice",
  "version": "1.0.0",
  "description": "get closed answers to each question",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prettier": "npx prettier --write .",
    "lint": "eslint --ext .ts --config .eslintrc . --fix",
    "lint:fix": "npm run prettier && npm run lint",
    "build": "tsc",
    "serve": "tsc-watch --onSuccess \"node dist/index.js\"",
    "start": "npm run build && npm run serve",
    "start:prod": "npm run build && node dist/index.js"
  },
  "author": "joaquin gonzalez budino",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "moment": "^2.29.4",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/express-winston": "^4.0.0",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.11.18",
    "@types/uuid": "^8.3.4",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "2.7.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.9.4"
  }
}
